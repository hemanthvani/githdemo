<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite" parallel="tests">
<listeners>
<listener class-name="Academy.Listeners"/>
</listeners>

	<test thread-count="5" name="App Test">
		<classes>
			<class name="Academy.AppTest" />
		</classes>
	</test> <!-- Test -->


	<test thread-count="5" name="ValidatePage Logo Test">
		<classes>
			<class name="Academy.validatePageLogo" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="HomePage Test">
		<classes>
			<class name="Academy.HomePage" />
		</classes>
	</test> <!-- Test -->
	
	<test thread-count="5" name="ValidateTitle Test">
		<classes>
			<class name="Academy.validateTitle" />
		</classes>
	</test> <!-- Test -->
	
</suite> <!-- Suite -->
